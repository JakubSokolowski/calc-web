        "ci:build": "nx affected:build --all",
        "all:test": " nx affected:test --all",
        "all:build": "nx affected:build --all",
        "deploy": "gh-pages -d dist/apps/calc-web",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "update": "nx migrate latest",
        "prod:stats": "nx build calc-web --prod --skip-nx-cache -- --statsJson",
